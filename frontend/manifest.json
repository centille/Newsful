{
  "manifest_version": 2,
  "name": "Extract",
  "version": "1.0",
  "description": "Extract text",
  "permissions": ["activeTab", "tabs", "desktopCapture"],
  "browser_action": {
    "default_popup": "./popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["./popup.js"]
    }
  ]
}
